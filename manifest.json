{
  "manifest_version": 2,
  "name": "Calz Tab 2017",
  "description": "My custom new tab page.",
  "homepage_url": "https://www.ellzzoo.com/",
  "version": "0.1",
  "icons": {
    "48": "images/tiger48.png",
    "128": "images/tiger128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["js/events.js"]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage", "bookmarks", "history", "<all_urls>", "geolocation"],
  "options_page": "opt/options.html"
}